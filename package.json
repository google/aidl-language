{
    "name": "aidl-language",
    "displayName": "AIDL",
    "description": "Syntax highlighting for the Android Interface Definition Language.",
    "version": "0.1.2",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "AIDL",
        "Android"
    ],
    "publisher": "Google",
    "repository": {
        "type": "git",
        "url": "https://github.com/google/aidl-language.git"
    },
    "license": "Apache-2.0",
    "contributes": {
        "languages": [
            {
                "id": "aidl",
                "aliases": [
                    "AIDL",
                    "aidl"
                ],
                "extensions": [
                    ".aidl"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "aidl",
                "scopeName": "source.aidl",
                "path": "./syntaxes/aidl.tmLanguage.json"
            }
        ]
    },
    "devDependencies": {
        "js-yaml": "^4.1.0"
    }
}